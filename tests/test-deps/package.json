{
  "name": "test-deps",
  "description": "soundworks application",
  "authors": [],
  "license": "BSD-3-Clause",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm run clean && sass src/styles:.build/public/css && soundworks-build -b",
    "clean": "soundworks-build -D",
    "dev": "npm run build && (concurrently -i -p \"none\" \"npm run watch:inspect server\" \"soundworks-build -b -w\" \"npm run watch:sass\")",
    "lint": "npx eslint src",
    "start": "node .build/server/index.js",
    "watch": "soundworks-build -p",
    "watch:inspect": "soundworks-build -i -p",
    "watch:sass": "sass --watch src/styles:.build/public/css"
  },
  "dependencies": {
    "@ircam/devicemotion": "latest",
    "@ircam/sc-components": "latest",
    "@ircam/sc-scheduling": "latest",
    "@ircam/sc-utils": "latest",
    "@soundworks/core": "latest",
    "@soundworks/helpers": "latest",
    "@soundworks/plugin-checkin": "latest",
    "@soundworks/plugin-filesystem": "latest",
    "@soundworks/plugin-logger": "latest",
    "@soundworks/plugin-platform-init": "latest",
    "@soundworks/plugin-position": "latest",
    "@soundworks/plugin-scripting": "latest",
    "@soundworks/plugin-sync": "latest",
    "lit": "^3.1.3",
    "node-web-audio-api": "latest"
  },
  "devDependencies": {
    "@ircam/eslint-config": "^2.0.1",
    "@soundworks/build": "file:../../",
    "@soundworks/create": "latest",
    "concurrently": "^8.2.2",
    "eslint": "^9.18.0",
    "sass": "^1.77.1"
  }
}
